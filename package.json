{
  "name": "@nekottyo/renovate-config",
  "version": "0.2.77",
  "description": "",
  "renovate-config": {
    "default": {
      "extends": [
        "config:base",
        "schedule:nonOfficeHours",
        "docker:enableMajor",
        ":automergeMinor",
        ":automergeRequireAllStatusChecks",
        ":label(renovate)",
        ":timezone(Asia/Tokyo)"
      ]
    }
  },
  "scripts": {
    "test": "renovate-config-validator",
    "release": "release-it"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nekottyo/renovate-config.git"
  },
  "author": "nekottyo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nekottyo/renovate-config/issues"
  },
  "homepage": "https://github.com/nekottyo/renovate-config#readme",
  "devDependencies": {
    "release-it": "13.6.9",
    "renovate": "21.33.15"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/",
    "access": "public"
  },
  "release-it": {
    "git": {
      "commitMessage": "[scip-ci] chore: release v${version}"
    },
    "github": {
      "release": true
    }
  }
}
